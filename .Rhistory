library("dplyr")
library(ggplot2)
data_frame <- read.csv( "owid-covid-data.csv", header = TRUE, sep = ",")
data_frame[is.na(data_frame)] <- 0 # replace NA with 0
countries <- c("Australia", "China", "India","New Zealand","Sweden","Ukraine","United Kingdom","United States")
ggplot(data_frame, aes(date, total_cases, group=factor(location))) + geom_line()
ggplot(data_frame, aes(x = date, y = total_cases, col = location)) +           # Draw line plot with ggplot2
geom_line()
ggplot(data_frame, aes(x = date, y = total_cases)) +           # Draw line plot with ggplot2
geom_line()
ggplot(data_frame, aes(x = date, y = total_cases)) +           # Draw line plot with ggplot2
geom_line()
ggplot(data_frame, aes(x = date, y = total_cases, col = countries)) +           # Draw line plot with ggplot2
geom_line()
ggplot(data_frame[,countries], aes(date, total_cases, group=factor(location))) + geom_line()
ggplot(data_frame[, countries], aes(date, total_cases, group=factor(location))) + geom_line()
